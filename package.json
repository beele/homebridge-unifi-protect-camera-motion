{
    "name": "homebridge-unifi-protect-camera-motion",
    "version": "0.4.3-beta.11",
    "description": "Unifi Protect cameras & motion sensors for Homekit with Homebridge",
    "main": "src/index.js",
    "scripts": {
        "postinstall": "node scripts/postinstall.js",
        "prepublishOnly": "node --max-old-space-size=1024 ./node_modules/typescript/lib/tsc.js",
        "compile": "node --max-old-space-size=1024 ./node_modules/typescript/lib/tsc.js",
        "homebridge": "tsc && homebridge -C -D -P . -U ./resources/test-config/",
        "watch": "nodemon",
        "test": "jest --config jest.config.js --collect-coverage"
    },
    "config": {
        "unsafe-perm": true
    },
    "license": "ISC",
    "keywords": [
        "homebridge-plugin"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/beele/homebridge-unifi-protect-camera-motion.git"
    },
    "bugs": {
        "url": "http://github.com/beele/homebridge-unifi-protect-camera-motion/issues"
    },
    "funding": {
        "type": "paypal",
        "url": "https://paypal.me/MrBeele?locale.x=nl_NL"
    },
    "engines": {
        "node": ">=10.17.0",
        "homebridge": ">=1.0.0"
    },
    "dependencies": {
        "@tensorflow/tfjs-node": "^2.8.6",
        "canvas": "^2.6.1",
        "execa": "^5.0.0",
        "ffmpeg-for-homebridge": "^0.0.9",
        "get-port": "^5.1.1",
        "googleapis": "67.1.0",
        "googlephotos": "^0.3.4",
        "node-fetch": "^2.6.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.31",
        "homebridge": "^1.3.1",
        "nodemon": "^2.0.7",
        "typescript": "^4.2.2",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.2"
    }
}
